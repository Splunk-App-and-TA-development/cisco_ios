<?xml version="1.0" encoding="utf-8"?><form>
  <label>DOT1X</label>
  <fieldset autoRun="true">
    <input type="time">
      <default>Last 60 minutes</default>
    </input>
  </fieldset>
    <row>
    <html>
      <section>
      <p>Add the following:</p>
      <ul>
        <li># of clients total, per switch, per protocol (802.1x/MAB/Webauth)</li>
        <li>Top client device type (if device sensor is enabled)</li>
        <li>Top user authentication</li>
        <li># of success/failed auth timeline</li>
        <li># of connected clients timeline</li>
      </ul>
      </section>
    </html>
    </row>
  <row>
    <panel>
      <table>
        <title>Authentications</title>
        <searchString>sourcetype=cisco:ios eventtype="cisco_ios-dot1x" | `normalize-int(src_int_prefix_long,src_int_suffix,"src_interface")`| transaction audit_session_id| table _time,host,mnemonic,audit_session_id,authenticator,src_interface,src_mac,src_vlan,result | stats latest(audit_session_id) AS audit_session_id, latest(authenticator) AS authenticator, latest(src_vlan) AS src_vlan, latest(result) AS result BY host,src_interface  | stats count(eval(result="succeeded" OR result="successful")) AS succeeded, count(eval(result="failed")) AS failed</searchString>
        <earliestTime>$earliest$</earliestTime>
        <latestTime>$latest$</latestTime>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>DOT1X events (will be made into transactions)</title>
        <searchString>sourcetype=cisco:ios eventtype="cisco_ios-dot1x" | `normalize-int(src_int_prefix_long,src_int_suffix,"src_interface")` | table _time,host,mnemonic,audit_session_id,authenticator,src_interface,src_mac,src_vlan,result</searchString>
        <earliestTime>$earliest$</earliestTime>
        <latestTime>$latest$</latestTime>
        <option name="count">20</option>
        <option name="displayRowNumbers">true</option>
        <option name="wrap">true</option>
        <option name="rowNumbers">true</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">row</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Audit Session ID transactions</title>
        <searchString>sourcetype=cisco:ios eventtype="cisco_ios-dot1x" | `normalize-int(src_int_prefix_long,src_int_suffix,"src_interface")` | transaction audit_session_id | table _time,host,mnemonic,audit_session_id,authenticator,src_interface,src_mac,src_vlan,result</searchString>
        <earliestTime>$earliest$</earliestTime>
        <latestTime>$latest$</latestTime>
        <option name="count">20</option>
        <option name="displayRowNumbers">true</option>
        <option name="wrap">true</option>
        <option name="rowNumbers">true</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">row</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>MAC address transactions</title>
        <searchString>sourcetype=cisco:ios eventtype="cisco_ios-dot1x" | `normalize-int(src_int_prefix_long,src_int_suffix,"src_interface")` | transaction src_mac | table _time,host,mnemonic,audit_session_id,authenticator,src_interface,src_mac,src_vlan,result</searchString>
        <earliestTime>$earliest$</earliestTime>
        <latestTime>$latest$</latestTime>
        <option name="count">20</option>
        <option name="displayRowNumbers">true</option>
        <option name="wrap">true</option>
        <option name="rowNumbers">true</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">row</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Audit session ID/MAC transactions</title>
        <searchString>sourcetype=cisco:ios eventtype="cisco_ios-dot1x" | `normalize-int(src_int_prefix_long,src_int_suffix,"src_interface")`| transaction audit_session_id src_mac | table _time,host,mnemonic,audit_session_id,authenticator,src_interface,src_mac,src_vlan,result</searchString>
        <earliestTime>$earliest$</earliestTime>
        <latestTime>$latest$</latestTime>
        <option name="count">20</option>
        <option name="displayRowNumbers">true</option>
        <option name="wrap">true</option>
        <option name="rowNumbers">true</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">row</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>MAC/IP transactions</title>
        <searchString>sourcetype=cisco:ios eventtype="cisco_ios-dot1x" | `normalize-int(src_int_prefix_long,src_int_suffix,"src_interface")` | transaction src_mac src_interface | table _time,host,mnemonic,audit_session_id,authenticator,src_interface,src_mac,src_vlan,result</searchString>
        <earliestTime>$earliest$</earliestTime>
        <latestTime>$latest$</latestTime>
        <option name="count">20</option>
        <option name="displayRowNumbers">true</option>
        <option name="wrap">true</option>
        <option name="rowNumbers">true</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">row</option>
      </table>
    </panel>
  </row>
</form>
