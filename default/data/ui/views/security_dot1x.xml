<?xml version='1.0' encoding='utf-8'?>
<form>
  <label>DOT1X</label>
  <fieldset autoRun="true">
    <input type="time">
      <default>Last 60 minutes</default>
    </input>
  </fieldset>
  <row>
    <table>
      <searchString>sourcetype=cisco:ios eventtype="cisco_ios-dot1x" | `normalize-int(int_prefix_long,int_suffix,"src")` | table _time,host,mnemonic,audit_session_id,authenticator,src_int_normalized,src_mac,src_vlan,result</searchString>
      <title>DOT1X events (will be made into transactions)</title>
      <option name="count">20</option>
      <option name="displayRowNumbers">true</option>
    </table>
  </row>
  <row>
    <table>
      <searchString>sourcetype=cisco:ios eventtype="cisco_ios-dot1x" | `normalize-int(int_prefix_long,int_suffix,"src")` | transaction audit_session_id | table _time,host,mnemonic,audit_session_id,authenticator,src_int_normalized,src_mac,src_vlan,result</searchString>
      <title>Audit Session ID transactions</title>
      <option name="count">20</option>
      <option name="displayRowNumbers">true</option>
    </table>
  </row>
  <row>
    <table>
      <searchString>sourcetype=cisco:ios eventtype="cisco_ios-dot1x" | `normalize-int(int_prefix_long,int_suffix,"src")` | transaction src_mac | table _time,host,mnemonic,audit_session_id,authenticator,src_int_normalized,src_mac,src_vlan,result</searchString>
      <title>MAC address transactions</title>
      <option name="count">20</option>
      <option name="displayRowNumbers">true</option>
    </table>
  </row>
  <row>
    <table>
      <searchString>sourcetype=cisco:ios eventtype="cisco_ios-dot1x" | `normalize-int(int_prefix_long,int_suffix,"src")`| transaction audit_session_id src_mac | table _time,host,mnemonic,audit_session_id,authenticator,src_int_normalized,src_mac,src_vlan,result</searchString>
      <title>Audit session ID/MAC transactions</title>
      <option name="count">20</option>
      <option name="displayRowNumbers">true</option>
    </table>
  </row>
  <row>
    <table>
      <searchString>sourcetype=cisco:ios eventtype="cisco_ios-dot1x" | `normalize-int(int_prefix_long,int_suffix,"src")` | transaction src_mac src_int | table _time,host,mnemonic,audit_session_id,authenticator,src_int_normalized,src_mac,src_vlan,result</searchString>
      <title>MAC/IP transactions </title>
      <option name="count">20</option>
      <option name="displayRowNumbers">true</option>
    </table>
  </row>
</form>
