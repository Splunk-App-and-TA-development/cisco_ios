<?xml version='1.0' encoding='utf-8'?>
<form>
  <label>DOT1X</label>
  <fieldset autoRun="true">
    <input type="time">
      <default>Last 60 minutes</default>
    </input>
  </fieldset>
  <row>
    <table>
      <searchString>sourcetype=cisco:ios eventtype="cisco_ios-dot1x"</searchString>
      <title>DOT1X events (will be made into transactions)</title>
      <option name="count">20</option>
      <option name="displayRowNumbers">true</option>
    </table>
  </row>
  <row>
    <table>
      <searchString>sourcetype=cisco:ios eventtype="cisco_ios-dot1x" | transaction audit_session_id</searchString>
      <title>Audit Session ID transactions</title>
      <option name="count">20</option>
      <option name="displayRowNumbers">true</option>
    </table>
  </row>
  <row>
    <table>
      <searchString>sourcetype=cisco:ios eventtype="cisco_ios-dot1x" | transaction src_mac</searchString>
      <title>MAC address transactions</title>
      <option name="count">20</option>
      <option name="displayRowNumbers">true</option>
    </table>
  </row>
  <row>
    <table>
      <searchString>sourcetype=cisco:ios eventtype="cisco_ios-dot1x" | transaction audit_session_id src_mac</searchString>
      <title>Audit session ID/MAC transactions</title>
      <option name="count">20</option>
      <option name="displayRowNumbers">true</option>
    </table>
  </row>
  <row>
    <table>
      <searchString>sourcetype=cisco:ios eventtype="cisco_ios-dot1x" | transaction src_mac src_int</searchString>
      <title>MAC/IP transactions </title>
      <option name="count">20</option>
      <option name="displayRowNumbers">true</option>
    </table>
  </row>
</form>
