<?xml version="1.0" encoding="utf-8"?><form>
  <label>Wireless - Dashboard</label>
  <fieldset autoRun="true">
    <input type="time">
      <default>Last 60 minutes</default>
    </input>
  </fieldset>
  <row>
    <panel>
      <html>
      <section>
      <p>Add the following:</p>
      <ul>
        <li># of clients total, per AP (and/or AP group), per band, per controller, per WLAN, per protocol (PSK/802.1x/MAB/Webauth)</li>
        <li>Top client device type (if device sensor is enabled)</li>
        <li>Top client application type (if AVP is enabled)</li>
        <li># of clients excluded with reason</li>
        <li>Top user authentication</li>
        <li># of success/failed auth timeline</li>
        <li># of connected clients timeline</li>
        <li>RF events chart with drill down</li>
        <li>Mobility events chart with drill down</li>
        <li>Top AP</li>
      </ul>
      </section>
    </html>
    </panel>
  </row>
  <row>
    <panel>
      <chart>
        <title>Events over time by access point MAC</title>
        <search>
          <query>sourcetype=cisco:ios ap_mac=* | timechart count BY ap_mac</query>
        </search>
        <option name="charting.chart">column</option>
        <option name="charting.axisY2.enabled">false</option>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">collapsed</option>
        <option name="charting.axisTitleY.visibility">visible</option>
        <option name="charting.axisTitleY2.visibility">visible</option>
        <option name="charting.axisX.scale">linear</option>
        <option name="charting.axisY.scale">linear</option>
        <option name="charting.axisY2.scale">inherit</option>
        <option name="charting.chart.bubbleMaximumSize">50</option>
        <option name="charting.chart.bubbleMinimumSize">10</option>
        <option name="charting.chart.bubbleSizeBy">area</option>
        <option name="charting.chart.nullValueMode">gaps</option>
        <option name="charting.chart.sliceCollapsingThreshold">0.01</option>
        <option name="charting.chart.stackMode">stacked</option>
        <option name="charting.chart.style">shiny</option>
        <option name="charting.drilldown">all</option>
        <option name="charting.layout.splitSeries">0</option>
        <option name="charting.legend.labelStyle.overflowMode">ellipsisMiddle</option>
        <option name="charting.legend.placement">right</option>
      </chart>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <searchString>eventtype="cisco_ios-lwapp_radio_crash" | table _time,host,radio_id,ap_mac</searchString>
        <title>Radio crashes</title>
        <option name="count">20</option>
        <option name="displayRowNumbers">true</option>
      </table>
    </panel>
    <panel>
      <table>
        <searchString>eventtype="cisco_ios-lwapp_akita_err" | table _time,host,ap_mac</searchString>
        <title>Unsupported access points</title>
        <option name="count">20</option>
        <option name="displayRowNumbers">true</option>
      </table>
    </panel>
  </row>
  <row>
    <panel>
      <table>
        <title>Events with MAC addresses</title>
        <search>
          <query>sourcetype=cisco:ios filename=* filename_line=* | rex field=message_text "(?&lt;src_mac&gt;[0-9a-fA-F]{2}:[0-9a-fA-F]{2}:[0-9a-fA-F]{2}:[0-9a-fA-F]{2}:[0-9a-fA-F]{2})" | rex field=message_text mode=sed "s/([[:xdigit:]]{1,2}:){5}[[:xdigit:]]{1,2}/XX:XX:XX:XX:XX/g" | stats latest(_time) AS _time, count by dvc,facility,mnemonic,src_mac,message_text,vendor_explanation | sort -count | table _time dvc facility mnemonic src_mac message_text vendor_explanation count</query>
        </search>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="drilldown">cell</option>
        <option name="dataOverlayMode">none</option>
        <option name="count">10</option>
      </table>
    </panel>
  </row>
</form>