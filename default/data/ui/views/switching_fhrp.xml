<?xml version='1.0' encoding='utf-8'?>
<form>
  <label>First Hop Redundancy Protocols</label>
  <fieldset autoRun="true">
    <input type="time">
      <default>Last 60 minutes</default>
    </input>
  </fieldset>
  <row>
    <table>
      <searchString>sourcetype=cisco:ios eventtype="cisco_ios-standby"</searchString>
      <title>HSRP</title>
      <option name="count">10</option>
      <option name="displayRowNumbers">true</option>
    </table>
  </row>
</form>
